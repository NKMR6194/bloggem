    <div class="page-header">
        <h1><%= @listTitle %></h1>
    </div>
    <div style="padding-bottom:10px;">
        <a href="./new/"><button type="button" class="btn btn-default btn-lg"><%= @addButton %></button></a>
    </div>
    <div class="list-group">
    <% @element.each do |e| %>
    <% e.title.gsub!('<', '&lt') %>
        <a href="./<%= e.id %>/" class="list-group-item"><%= e.title %></a>
    <% end %>
    </div>
